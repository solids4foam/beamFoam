FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      DTheta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    // Bottom edge (clamped)
    left
    {
        type fixedValue;
        value uniform (0 0 0);
    }

    // Top edge (free)
    right
    {
        type momentBeamRotationNR;
        momentSeries
        {
            "fileName|file"        "$FOAM_CASE/constant/timeVsMoment";
            outOfBounds     clamp;
        }        
    }

    beam
    {
        type empty;
    }
}


