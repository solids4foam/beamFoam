beamModel coupledTotalLagNewtonRaphsonBeam;

beams
(
    beam_0
    {
	    crossSectionModel rectangle;
	
	    rectangleCrossSectionModelDict
        {
            b 0.2;
            h 0.2;
            nx 1;
            ny 1;
        }

        length 2;
        nSegments 40;
        E  15.293e6;
        G    5.882e6;
        rho    1000;
    }
);

"coupledTotalLagNewtonRaphsonBeamCoeffs"
{
    // rho rho [1 -3 0 0 0 0 0] 1000;
    // E E [1 -1 -2 0 0 0 0] 1;
    // G G [1 -1 -2 0 0 0 0] 5.882e6;
    
    nCorrectors 200;
    solutionTol 1e-10;

    debug no;
}
