FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      solidProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// linearGeometry: assumes small strains and rotations
solidModel     coupledTotalLagNewtonRaphsonBeam;

beamModelCoeffs
{
    // Mechanical properties
    #include "../../mechanical.dat"

    // Length
    #include "../../length.dat";

    // Density
    rho rho [1 -3 0 0 0 0 0] 1030;

    // End patches of beam    
    startPatchName left;
    endPatchName right;

    // Number of correctors
    nCorrectors 1000;

    // Soution convergence tolerance
    convergenceTol 1e-09;

    // Debug flag
    debug false;

    // Flag to use Updated Lagrangian, otherwise, small strain is assumed
    updatedLagrangian yes;

    // Static (true) or dynamic (false)
    steadyState false;

    // Maximum number of momentum correctors
    nCorrectors     50;

    // Solution tolerance for displacement
    solutionTolerance 1e-08;

    // Alternative solution tolerance for displacement
    alternativeTolerance 1e-08;

    // Material law solution tolerance
    materialTolerance 1e-08;

    // Write frequency for the residuals
    infoFrequency   100;
}


