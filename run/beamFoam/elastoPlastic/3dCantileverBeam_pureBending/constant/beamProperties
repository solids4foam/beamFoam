FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      beamProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

beamModel coupledTotalLagNewtonRaphsonBeam;

nBeams 1;
R R [0 1 0 0 0 0 0] 0.5; // mm;
L L [0 1 0 0 0 0 0] 200; // mm
nSegments 100;

coupledTotalLagNewtonRaphsonBeamCoeffs
{
    E E [1 -1 -2 0 0 0 0] 200000;
    G G [1 -1 -2 0 0 0 0] 100000; // nu=0

    plasticityModel axialForce3dBending;
    axialForce3dBendingCoeffs
    {
        sigmaY0 sigmaY0 [1 -1 -2 0 0 0 0] 700;
        Ep Ep [1 -1 -2 0 0 0 0] 20000;
    }
    
    startPatchName left;
    endPatchName right;

    nCorrectors 1000;
    convergenceTol 1e-10;

    debug yes;
    updatedLagrangian no;
}

