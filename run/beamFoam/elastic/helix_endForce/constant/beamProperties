FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      beamProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

beamModel coupledTotalLagNewtonRaphsonBeam;

nBeams 1;
R R [0 1 0 0 0 0 0] 0.570732; //5.70732;
L L [0 1 0 0 0 0 0] 1000;
nSegments 1000;

coupledTotalLagNewtonRaphsonBeamCoeffs
{
    E E [1 -1 -2 0 0 0 0] 1;
    G G [1 -1 -2 0 0 0 0] 0.5; // nu=0
    
    startPatchName left;
    endPatchName right;

    nCorrectors 1000;
    convergenceTol 1e-10;

    debug no;
    updatedLagrangian yes;
}

