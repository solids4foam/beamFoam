FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      DTheta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    right
    {
        type fixedValue;
        value uniform (0 0.01745 0);
    }

    left
    {
        type momentBeamRotation;
        moment uniform (0 0 0);
        value uniform (0 0 0);
        // momentSeries
        // {
        //     fileName        "$FOAM_CASE/constant/timeVsMoment";
        //     outOfBounds     clamp;
        // }        
    }

    frontAndBack
    {
        type empty;
    }

    topAndBottom
    {
        type empty;
    }
}


