FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceVectorField;
    location    "0";
    object      dRuDs;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    left
    {
        type            calculated;
        value           uniform (-1 -0 -0);
    }
    right
    {
        type            calculated;
        value           uniform (1 0 0);
    }
    frontAndBack
    {
        type            empty;
    }
    topAndBottom
    {
        type            empty;
    }
}


