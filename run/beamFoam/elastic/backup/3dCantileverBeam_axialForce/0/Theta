FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      DTheta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
        type fixedValue;
        value uniform (0 0 0);
    }

    // right
    // {
    //     type timeVaryingUniformFixedValue;
    //     fileName "$FOAM_CASE/constant/timeVsTheta";
    //     outOfBounds clamp;           // (error|warn|clamp|repeat)
    // }

    // right
    // {
    //     type fixedValue;
    //     value uniform (0 -0.239285 0.239285);
    // }
    
    right
    {
        type momentBeamRotationNR;
        momentSeries
        {
            fileName        "$FOAM_CASE/constant/timeVsMoment";
            outOfBounds     clamp;
        }        
    }

    frontAndBack
    {
        type empty;
    }

    topAndBottom
    {
        type empty;
    }
}


