FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      W;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
        type fixedValue;
        value uniform (0 0 0);
    }

    right
    {
        type followerForceBeamDisplacementNR;

        followerForceSeries
        {
            fileName "$FOAM_CASE/constant/timeVsFollowerForce";
            outOfBounds clamp;
        }
    }
    
    beam
    {
        type empty;
    }
}


