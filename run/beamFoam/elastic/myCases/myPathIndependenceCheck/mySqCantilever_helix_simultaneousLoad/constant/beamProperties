FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      beamProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

beamModel coupledTotalLagNewtonRaphsonBeam;

// This test case is setup as per Meier's example 4 problem; 
// verification of path-independence; slenderness ratio = 100

beams
(
beam_0
{
        crossSectionModel rectangle;

        rectangleCrossSectionModelDict
        {
        
            b 0.1;
            h 0.1;
            nx 4;
            ny 4;
        }

        length 1000;
        nSegments 20;
}
);
//nBeams 1;
//R R [0 1 0 0 0 0 0] 0.02; //5.70732;
//L L [0 1 0 0 0 0 0] 10;
//nSegments 100;


"coupledTotalLagNewtonRaphsonBeamCoeffs"
{
    E E [1 -1 -2 0 0 0 0] 1;//20e6;
    G G [1 -1 -2 0 0 0 0] 0.5;//10e6; // nu=0
    
    startPatchName left;
    endPatchName right;

    nCorrectors 200;
    convergenceTol 1e-10;

    debug yes;
    
    updatedLagrangian no;

    ibrahimovicCase false;
    
    objectiveInterpolation true;
}

