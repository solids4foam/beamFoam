FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceVectorField;
    location    "0";
    object      refTangent;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   nonuniform List<vector> 7((0.995185 0.0980172 0) (0.980785 0.195091 0) (0.95694 0.290284 0) (0.92388 0.382683 0) (0.881921 0.471397 0) (0.831469 0.555571 0) (0.773011 0.634393 0));

boundaryField
{
    left
    {
        type            calculated;
        value           uniform (1 0 0);
    }
    right
    {
        type            calculated;
        value           uniform (0.707107 0.707107 0);
    }
    beam
    {
        type            empty;
    }
}


