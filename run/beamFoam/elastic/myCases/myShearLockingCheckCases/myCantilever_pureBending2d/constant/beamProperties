R R [0 1 0 0 0 0 0] 0.1; //5.70732;
L L [0 1 0 0 0 0 0] 1;
nSegments 5;
*/
beams
(
beam_0
{
        crossSectionModel rectangle;

        rectangleCrossSectionModelDict
        {
        
            b	0.1;
            h	0.1;
            nx	1;
            ny	1;
        }

        length 1;
        nSegments 5;
}
);

"coupledTotalLagNewtonRaphsonBeamCoeffs"
{
    E E [1 -1 -2 0 0 0 0] 1;//20e6;
    G G [1 -1 -2 0 0 0 0] 0.5;//10e6; // nu=0
    
    startPatchName left;
    endPatchName right;

    nCorrectors 200;
    convergenceTol 1e-6;

    debug no;
    
    updatedLagrangian no;

    ibrahimovicCase true;
    
}

