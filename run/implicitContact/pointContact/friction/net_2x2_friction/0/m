FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      m;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    "left_0|left_1|left_2|left_3"
    {
        type calculated;
        value uniform (0 0 0);
    }

    "right_0|right_1|right_2|right_3"
    {
        type calculated;
        value uniform (0 0 0);
    }

    beam
    {
        type empty;
    }
}


