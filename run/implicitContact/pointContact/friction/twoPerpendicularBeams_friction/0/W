FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      DW;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    "left_0|right_0"
    {
        type fixedDisplacement;
        value uniform (0 0 0);
    }

    "left_1|right_1"
    {
        type fixedDisplacement;
        value uniform (0 0 0);
        displacementSeries
        {
            fileName        "$FOAM_CASE/constant/timeVsDisplacement";
            outOfBounds     clamp;
        }
    }
    
    beam
    {
        type empty;
    }
}


