FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      beamsBunchingProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

lineContact
{
    //   This lineContact property will not be used here
    //   because its fully in point contact regime 
    //   but still it needs to be specified
    normalContactModel standardPenalty;
    standardPenaltyNormalModelDict
    {
	epsilon 1000; // Set to 10 for agumentedLagrangian
     //   gbar 0.001;
    }

    frictionContactModel standardPenalty;
    standardPenaltyFrictionModelDict
    {} //frictionless
}

pointContact
{
    normalContactModel standardPenalty;
    standardPenaltyNormalModelDict
    {
	epsilon 6000; // Set to 10 for agumentedLagrangian
     //   gbar 0.001;
    }

    frictionContactModel standardPenalty;
    standardPenaltyFrictionModelDict
    {} //frictionless
}


agumentedLagrangian	false;
reusePrevPointContacts		true;

// debug yes;

// ************************************************************************* //
