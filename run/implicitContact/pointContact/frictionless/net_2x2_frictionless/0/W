FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      DW;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    "left_0|left_1"
    {
        type fixedValue;
        value uniform (0 0 0);
    }

    "right_0|right_1"
    {
        type fixedValue;
        value uniform (0 0 0);
    }

    "left_2|left_3"
    {
        type fixedDisplacement;
        value uniform (0 0 0);
        
        displacementSeries
        {
            fileName        "$FOAM_CASE/constant/timeVsDisplacement";
            outOfBounds     clamp;
        }
    }
    
    right_2
    {
        type axialForceTransverseDisplacementNR
        value uniform (0 0 0);
        // refDisp uniform (0 0 0);
        axialForce uniform 10;
          
        refDispSeries
        {
            fileName        "$FOAM_CASE/constant/timeVsRefDisplacement_2";
            outOfBounds     clamp;
        }
     }
    
    right_3
    {
        type axialForceTransverseDisplacementNR
        value uniform (0 0 0);
        // refDisp uniform (0 0 0);
        axialForce uniform 10;
          
        refDispSeries
        {
            fileName        "$FOAM_CASE/constant/timeVsRefDisplacement_3";
            outOfBounds     clamp;
        }
     }
    
    beam
    {
        type empty;
    }
}


