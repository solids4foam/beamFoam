FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      beamsBunchingProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
pointContact
{
    normalContactModel standardPenalty;
    standardPenaltyNormalModelDict
    {
	epsilon 10000;
    }

    frictionContactModel standardPenalty;
    standardPenaltyFrictionModelDict
    {}
}

lineContact
{
    normalContactModel standardPenalty;
    standardPenaltyNormalModelDict
    {
	epsilon 10000;
    }

    frictionContactModel standardPenalty;
    standardPenaltyFrictionModelDict
    {}
}


reusePrevPointContacts true;

