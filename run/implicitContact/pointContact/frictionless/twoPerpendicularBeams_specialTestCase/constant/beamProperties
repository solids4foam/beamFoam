FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      beamProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

beamModel coupledTotalLagNewtonRaphsonBeam;

beams
(
    beam_0
    {
	crossSectionModel	circle;
		
	circleCrossSectionModelDict	
	{
	    radius	0.005;
	    scalingArea 100;
	    scalingMI   100;
	}
		
	length	0.5;
	nSegments	20;
		
    }
	
    beam_1
    {
	crossSectionModel	circle;
			
	circleCrossSectionModelDict
	{
	    radius	0.005;
	    scalingArea 0.01;
	}
			
	length	0.5;
	nSegments	20;
    }		
	
);
// nBeams 2;
// R R [0 1 0 0 0 0 0] 0.005;
// L L [0 1 0 0 0 0 0] 0.5;
// nSegments 21;

coupledTotalLagNewtonRaphsonBeamCoeffs
{
    E E [1 -1 -2 0 0 0 0] 1.0e9;
    // G G [1 -1 -2 0 0 0 0] 0.333e9; // nu=0.5
    G G [1 -1 -2 0 0 0 0] 0.5e9; // nu=0
    
    startPatchName	left;
    endPatchName	right;

    nCorrectors	100;
    convergenceTol	1e-6;
    // relConvergenceTol 1e-4;

    debug	yes;

    updatedLagrangian	no;
}

