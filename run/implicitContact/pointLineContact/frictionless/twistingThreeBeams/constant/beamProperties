FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      beamProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

beamModel coupledTotalLagNewtonRaphsonBeam;

beams
(
    "beam_0|beam_1|beam_2|beam_3"
    {
	crossSectionModel	circle;
		
	circleCrossSectionModelDict	
	{
	    radius	0.01;
	}
		
	length	5;
	nSegments	100;
		
    }
	
);

//   nBeams 4;
//   R R [0 1 0 0 0 0 0] 0.01; //0.05;
//   L L [0 1 0 0 0 0 0] 5;
//   nSegments 100;

coupledTotalLagNewtonRaphsonBeamCoeffs
{
    E E [1 -1 -2 0 0 0 0] 1.0e9;
    G G [1 -1 -2 0 0 0 0] 0.5e9; // nu=0

    startPatchName left;
    endPatchName right;

    nCorrectors 2000;
    convergenceTol 1e-10;
    // relConvergenceTol 1e-6;

    debug false;

    updatedLagrangian yes;
}

