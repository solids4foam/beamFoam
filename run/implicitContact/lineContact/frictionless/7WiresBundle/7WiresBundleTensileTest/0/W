FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      DW;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
        type fixedValue;
        value uniform (0 0 0);
    }

    right
    {
        type fixedDisplacement;
        value uniform (0 0 0);
        
        displacementSeries
        {
            fileName        "$FOAM_CASE/constant/timeVsDisplacement";
            outOfBounds     clamp;
        }
    }
    
    // right
    // {
    //     type axialForceTransverseDisplacement;
    //     refDisp uniform ( 0 0 0 );
    //     value uniform (0 0 0);
    //     axialForce uniform 1000;
    // }
    
    beam
    {
        type empty;
    }
}


