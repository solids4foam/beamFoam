FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     beamFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         5; //1;

deltaT          0.005; //0.025;

writeControl    timeStep;

writeInterval   1; //2;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

