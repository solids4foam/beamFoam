functions
{
   setEndPointsDisplacement
   {
       type setEndPointsDisplacement;
       functionObjectLibs ("libsetEndPointsDisplacement.so");
       startPatchName left_1;
       endPatchName right_1;
       yDisp -0.3;
       angle 360; //1440; //720; //360;
   }
}

