FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      beamProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

beamModel coupledTotalLagNewtonRaphsonBeam;


beams
(
    beam_0
    {
        crossSectionModel polygon;

        polygonCrossSectionModelDict
        {
            nSides 6;
            minRadius 0.01;
            filletAngleDeg 20;

            nSegmentsPerSide 10;
            nRadialSegments 12;
        }

        length 5;
        nSegments 100;
    }
    
    beam_1
    {
        crossSectionModel polygon;

        polygonCrossSectionModelDict
        {
            nSides 6;
            minRadius 0.01;
            filletAngleDeg 20;

            nSegmentsPerSide 10;
            nRadialSegments 12;
        }

        length 5;
        nSegments 100;
    }
);


// R 2(0.01 0.01);
// L 2(5 5);
// U 2(0 0);
// nSegments 2(100 100);

"coupledTotalLagNewtonRaphsonBeamCoeffs"
{
    E E [1 -1 -2 0 0 0 0] 1.0e9;
    G G [1 -1 -2 0 0 0 0] 0.5e9; // nu=0
    
    startPatchName left;
    endPatchName right;

    nCorrectors 1000;
    convergenceTol 1e-10;
    // relConvergenceTol 1e-4;

    debug no;

    updatedLagrangian yes;
}



