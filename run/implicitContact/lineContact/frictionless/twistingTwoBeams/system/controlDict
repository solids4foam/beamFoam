FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     beamFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         1;

deltaT          0.001;

writeControl    timeStep;

writeInterval   10;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

functions
{
   setEndPointTwisting
   {
       type setEndPointTwisting;
       functionObjectLibs ("libsetEndPointTwisting.so");
       startPatchName left;
       endPatchName right;
       angle 360;
   }
  
   setCrossSectionRotation
   {
       type setCrossSectionRotation;
       functionObjectLibs ("libsetCrossSectionRotation.so");
       startPatchName left;
       endPatchName right;
       angle 360;
   }
}

