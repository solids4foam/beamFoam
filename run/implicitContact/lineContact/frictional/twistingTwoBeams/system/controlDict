FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     beamFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1;

deltaT          0.01;

writeControl    timeStep;

writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

functions
{
   setEndPointDisplacement_0
   {
       type setEndPointDisplacement;
       functionObjectLibs ("libsetEndPointDisplacement.so");
       startPatchName left_0;
       endPatchName right_0;
       angle 360; //1440; //720; //360;
   }
   setEndPointDisplacement_1
   {
       type setEndPointDisplacement;
       functionObjectLibs ("libsetEndPointDisplacement.so");
       startPatchName left_1;
       endPatchName right_1;
       angle 360; //1440; //720; //360;
   }
}

