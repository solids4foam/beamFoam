set terminal postscript color solid
set output "displacementVsForce.ps"


set xlabel "Axial displacement"
set ylabel  "Axial force"

set grid
#set size ratio -1
set size 0.7,0.7

set border linewidth 1.5

#set rmargin 2
#set lmargin 6

#set yrange [-1:0]

set key top left

plot [0:] "history/0/forceMoment_right.dat" using (0.04*$1):(1.1*$2) title "frictionl (mu=0.3)" with lines lw 2, \
"history_frictionless/0/forceMoment_right.dat" using (0.04*$1):2 title "frictionless" with lines lw 2

