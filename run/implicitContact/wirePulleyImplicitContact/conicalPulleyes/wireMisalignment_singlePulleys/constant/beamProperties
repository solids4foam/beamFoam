FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      beamProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

beamModel coupledTotalLagNewtonRaphsonBeam;

nBeams 1;
R R [0 1 0 0 0 0 0] 0.5; // mm;
L L [0 1 0 0 0 0 0] 2000; // mm
nSegments 2000;

"coupledTotalLagNewtonRaphsonBeamCoeffs"
{
    E E [1 -1 -2 0 0 0 0] 200e6;
    G G [1 -1 -2 0 0 0 0] 100e6; // nu=0

    // plasticityModel axialForce3dBending;
    // axialForce3dBendingCoeffs
    // {
    //     sigmaY0 sigmaY0 [1 -1 -2 0 0 0 0] 2.25e6;
    //     Ep Ep [1 -1 -2 0 0 0 0] 0;

    //     elPredTol 1e-4;
    // }

    startPatchName left;
    endPatchName right;

    nCorrectors 50;
    convergenceTol 1e-10;

    debug yes;
    updatedLagrangian no;
}

conicalPulleys
(
    pulley_0
    {
        origin (0 0 0);
        axis (0 0 1);
        polarAxis (1 0 0);
        rMin 150;
        angle 60;
        width 10;
        rotDir -1;

        // frictionless false;
    }
);


