"./3/conicalPulleyNormalContactForce.txt" using 1:4 axis x1y2 title "Fcn[0]" with lines, \
"./3/conicalPulleyNormalContactForce.txt" using 1:5 axis x1y2 title "Fcn[1]" with lines, \
"./3/beamShape.dat" using 1:4 axis x1y1 title "ThetaX" with lines


