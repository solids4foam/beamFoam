set output "beamThetaX.ps"

set xlabel "x, mm"
set ylabel  "ThetaX"
set y2label "Fcn"

set y2tics

set grid
#set size ratio -1

set border linewidth 1.5

#set rmargin 2
#set lmargin 6

#set yrange [-1:0]

set key top left

