        polarAxis (1 0 0);
        rMin 15;
        rToroidalSurf 150000;
        width 20;
        rotDir -1;
    }
    
    pulley_1
    {
        origin (32 0 0);
        axis (0 0 1);
        polarAxis (1 0 0);
