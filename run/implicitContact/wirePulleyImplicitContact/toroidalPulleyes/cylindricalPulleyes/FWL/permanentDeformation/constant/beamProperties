FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      beamProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


beams
(
    beam_0
    {
        crossSectionModel rectangle;

        rectangleCrossSectionModelDict
        {
            b 7; //mm
            h 0.9; //mm

            nx 4;
            ny 20;
        }

        length 400; // mm
        nSegments 400;
    }
);


nBeams 1;
R R [0 1 0 0 0 0 0] 0.45; // mm;
L L [0 1 0 0 0 0 0] 400; // mm
nSegments 400;


beamModel coupledTotalLagNewtonRaphsonBeam;

"coupledTotalLagNewtonRaphsonBeamCoeffs"
{
    E E [1 -1 -2 0 0 0 0] 200e6;
    G G [1 -1 -2 0 0 0 0] 100e6; // nu=0

    plasticityModel linearElasticMisesPlastic;
    linearElasticMisesPlasticCoeffs
    {
        sigmaY0 sigmaY0 [1 -1 -2 0 0 0 0] 1.65e6;
        HP EP [1 -1 -2 0 0 0 0] 0;

        elPredTol 1e-6;

        writeFaceIDs 2(286 309);
        plasticity false;
    }

    startPatchName left;
    endPatchName right;

    nCorrectors 2000;
    convergenceTol 1e-12;

    debug no;
    updatedLagrangian no;
}

