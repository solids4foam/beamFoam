FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      beamProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

beamModel coupledTotalLagNewtonRaphsonBeam;

nBeams 1;
R R [0 1 0 0 0 0 0] 0.65; // mm;
L L [0 1 0 0 0 0 0] 1000; // mm
nSegments 1000;

"coupledTotalLagNewtonRaphsonBeamCoeffs"
{
    E E [1 -1 -2 0 0 0 0] 200e6;
    G G [1 -1 -2 0 0 0 0] 100e6; // nu=0

    // plasticityModel axialForce3dBending;
    // axialForce3dBendingCoeffs
    // {
    //     sigmaY0 sigmaY0 [1 -1 -2 0 0 0 0] 2.25e6;
    //     Ep Ep [1 -1 -2 0 0 0 0] 0;

    //     elPredTol 1e-4;
    // }

    startPatchName left;
    endPatchName right;

    nCorrectors 1000;
    convergenceTol 1e-10;

    debug no;
    updatedLagrangian no;
}

toroidalPulleys
(
    // First set
    pulley_1
    {
        origin (0 0 0);
        axis (0 0 1);
        polarAxis (1 0 0);
        rMin 18;
        rToroidalSurf 10000;
        width 10;
        rotDir -1;
        meanBearingDiameter 16;
        bearingFrictionCoeff 0.02;
    }
    pulley_2
    {
        origin (60 0 0); //(50 25 0);
        axis (0 0 1);
        polarAxis (1 0 0);
        rMin 19.5;
        rToroidalSurf 10000;
        width 10;
        rotDir 1;
        meanBearingDiameter 16;
        bearingFrictionCoeff 0.02;
    }
    pulley_3
    {
        origin (125 -10 0);
        axis (0 0 1);
        polarAxis (1 0 0);
        rMin 25.5;
        rToroidalSurf 10000;
        width 10;
        rotDir -1;
        meanBearingDiameter 16;
        bearingFrictionCoeff 0.02;
    }
);


