"./1/conicalPulleyNormalContactForce.txt" using (-1*$1):4 axis x1y2 title "Fcn[0]" with lines, \
"./1/conicalPulleyNormalContactForce.txt" using (-1*$1):5 axis x1y2 title "Fcn[1]" with lines, \
"./1/beamShape.dat" using (-1*$1):4 axis x1y1 title "ThetaX" with lines


