set terminal postscript color solid
set output "beamTorsionAngle.ps"

set xlabel "x, mm"
set ylabel  "Torsion angle"
set y2label "Normal contact force (Fcn)"

set y2tics

set grid
#set size ratio -1

set border linewidth 1.5

#set rmargin 2
#set lmargin 6

set y2range [0:1000]

set key bottom left

plot [0:] \
"./0.88275/conicalPulleyNormalContactForce.txt" using 1:4 axis x1y2 title "Fcn 0" with lines lt 2 lw 2, \
"./0.88275/conicalPulleyNormalContactForce.txt" using 1:5 axis x1y2 title "Fcn 1" with lines lt 3 lw 2, \
"./0.88275/beamShape.dat" using 1:4 axis x1y1 title "Torsion angle" with lines lt 7 lw 4


