FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     beamFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         1; //1.02;

deltaT          0.0005; //0.00060606;

writeControl    timeStep;

writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;
// adjustTimeStep  yes;

libs
(
    "libblockMklParadisoSolver.so"
);


functions
{
   // patchForceMomentHistory
   // {
   //     type patchForceMomentHistory;
   //     functionObjectLibs ("libpatchForceMomentHistory.so");
   //     patchName right;
   // }

   conicalPulleyFrictionControl0
   {
       type conicalPulleyFrictionControl;
       functionObjectLibs ("libconicalPulleyFrictionControl.so");
       changeFrictionTime 1.002;
       pulleyIndices 1(0);
   }

   conicalPulleyFrictionControl1
   {
       type conicalPulleyFrictionControl;
       functionObjectLibs ("libconicalPulleyFrictionControl.so");
       changeFrictionTime 1.002; //1.00365;
       pulleyIndices 1(1);
   }

   conicalPulleyContactHistory
   {
       type conicalPulleyContactHistory;
       functionObjectLibs ("libconicalPulleyContactHistory.so");
       pulleyIndex 1;
       beamIndex 0;
       segmentIndex 0;
   }

   writeBeamData
   {
       type writeBeamData;
       functionObjectLibs ("libwriteBeamData.so");
       startPatchName left;
       endPatchName right;
   }
}

