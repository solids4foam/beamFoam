FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      beamProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

beamModel coupledTotalLagNewtonRaphsonBeam;

R R [0 1 0 0 0 0 0] 0.65; // mm;
L L [0 1 0 0 0 0 0] 1000; // mm
nSegments 1000;
nBeams 1;

"coupledTotalLagNewtonRaphsonBeamCoeffs"
{
    E E [1 -1 -2 0 0 0 0] 200e6;
    G G [1 -1 -2 0 0 0 0] 100e6; // nu=0

    plasticityModel axialForce3dBending;
    axialForce3dBendingCoeffs
    {
        sigmaY0 sigmaY0 [1 -1 -2 0 0 0 0] 2.25e6;
        Ep Ep [1 -1 -2 0 0 0 0] 2e2;

        elPredTol 1e-8;
    }

    startPatchName left;
    endPatchName right;

    nCorrectors 200;
    convergenceTol 1e-10;

    debug yes;
    updatedLagrangian no;
}

conicalPulleys
(
    // First set
    pulley_1
    {
        origin (0 0 0);
        axis (0 0 1);
        polarAxis (1 0 0);
        rMin 18;
        angle 45;
        width 10;
        rotDir -1;
        // meanBearingDiameter 16;
        // bearingFrictionCoeff 0.02;
    }
    pulley_2
    {
        origin (55 5 0); //(50 25 0);
        axis (0 0 1);
        polarAxis (1 0 0);
        rMin 19.5;
        angle 45;
        width 10;
        rotDir 1;
        // meanBearingDiameter 16;
        // bearingFrictionCoeff 0.02;
    }
    pulley_3
    {
        origin (120 -8 0);
        axis (0 0 1);
        polarAxis (1 0 0);
        rMin 25.5;
        angle 45;
        width 10;
        rotDir -1;
        // meanBearingDiameter 16;
        // bearingFrictionCoeff 0.02;
    }
    
    // Second set
    pulley_4
    {
        origin (0 0 0);
        originShift (205 18 18);
        axis (0 1 0);
        polarAxis (1 0 0);
        rMin 18;
        angle 45;
        width 10;
        rotDir -1;
        // meanBearingDiameter 16;
        // bearingFrictionCoeff 0.02;
    }
    pulley_5
    {
        origin (55 0 -5); //(50 25 0);
        originShift (205 18 18);
        axis (0 1 0);
        polarAxis (1 0 0);
        rMin 19.5;
        angle 45;
        width 10;
        rotDir 1;
        // meanBearingDiameter 16;
        // bearingFrictionCoeff 0.02;
    }
    pulley_4
    {
        origin (120 0 -8);
        originShift (205 18 18);
        axis (0 1 0);
        polarAxis (1 0 0);
        rMin 25.5;
        angle 45;
        width 10;
        rotDir -1;
        // meanBearingDiameter 16;
        // bearingFrictionCoeff 0.02;
    }
);


