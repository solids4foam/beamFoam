FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      beamProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

beamModel coupledTotalLagNewtonRaphsonBeam;

nBeams 1;
R R [0 1 0 0 0 0 0] 0.570732;//0.564189584; //0.570732; //0.5642; //0.570732; //unit square equivalent
L L [0 1 0 0 0 0 0] 157.07963; //78.54;
nSegments 10;

"coupledTotalLagNewtonRaphsonBeamCoeffs"
{
    E E [1 -1 -2 0 0 0 0] 1.0;
    G G [1 -1 -2 0 0 0 0] 0.5; // nu=0
    
    startPatchName left;
    endPatchName right;

    nCorrectors 200;
    convergenceTol 1e-10;

    debug yes;
    updatedLagrangian no;
    
    objectiveInterpolation false;
}

